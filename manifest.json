{
    "display_information": {
        "name": "DocuSign-bot",
        "description": "Sign contracts securely with the official DocuSign bot",
        "background_color": "#737373"
    },
    "features": {
        "bot_user": {
            "display_name": "DocuSign-bot",
            "always_online": false
        },
        "unfurl_domains": [
            "docusign.net",
            "docusign.com"
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "links:read",
                "links:write",
                "users:read",
                "channels:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://us-central1-extension-scraper.cloudfunctions.net/slack-demo-app",
            "bot_events": [
                "link_shared"
            ]
        },
        "interactivity": {
            "is_enabled": true,
            "request_url": "https://us-central1-extension-scraper.cloudfunctions.net/slack-demo-app"
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}